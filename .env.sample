# Contentful API keys
CONTENTFUL_SPACE_ID=
CONTENTFUL_ACCESS_TOKEN=

# -------------------------------
# Clerk Authentication Config
# -------------------------------

# Your Clerk frontend publishable key for client-side auth
# Get it from: https://dashboard.clerk.com
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=

# Your Clerk backend secret key for server-side auth
CLERK_SECRET_KEY=

# Custom route for Clerk sign-in page
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in

# Custom route for Clerk sign-up page
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up

# Where to redirect the user after signing in
NEXT_PUBLIC_CLERK_SIGN_IN_FALLBACK_REDIRECT_URL=/dashboard

# Where to redirect the user after signing up
NEXT_PUBLIC_CLERK_SIGN_UP_FALLBACK_REDIRECT_URL=/dashboard


# -------------------------------
# YouTube API
# -------------------------------

# API key to access YouTube Data API
# Enable API at: https://console.cloud.google.com
NEXT_PUBLIC_YOUTUBE_API_KEY=


# -------------------------------
# Supabase Database URLs
# -------------------------------

# Connection pooling URL for Supabase (used by the app in production)
# Get it from: https://supabase.com (Project → Settings → Database)
DATABASE_URL=

# Direct connection URL for Supabase (used by Prisma or migration tools)
DIRECT_URL=

# ============================================================
#  NOTE: Only ONE LLM provider is required.
#  Uncomment and fill in the section for the provider you want.
# ============================================================


# ------------------------------------------------------------
# Google Gemini
# Get API Key: https://aistudio.google.com/app/apikey
# Docs: https://ai.google.dev/
# ------------------------------------------------------------
# AI_PROVIDER=gemini
GEMINI_API_KEY=
GEMINI_MODEL=gemini-2.0-flash


# ------------------------------------------------------------
# DeepSeek
# Get API Key: https://platform.deepseek.com/api-keys
# Docs: https://platform.deepseek.com/
# ------------------------------------------------------------
# AI_PROVIDER=deepseek
DEEPSEEK_API_KEY=
DEEPSEEK_MODEL=deepseek-chat


# ------------------------------------------------------------
# OpenAI
# Get API Key: https://platform.openai.com/settings/organization/api-keys
# Docs: https://platform.openai.com/docs/
# ------------------------------------------------------------
# AI_PROVIDER=openai
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4.1-mini


# -------------------------------
# Cloudinary (for media uploads)
# -------------------------------

# Your Cloudinary API key
# Get it from: https://cloudinary.com (Dashboard)
CLOUDINARY_API_KEY=

# Your Cloudinary API secret
CLOUDINARY_API_SECRET=

# Your Cloudinary cloud name
CLOUDINARY_CLOUD_NAME=


# -------------------------------
# Twilio (for WhatsApp messaging)
# -------------------------------

# Twilio Account SID
# Get it from: https://www.twilio.com/console
TWILIO_ACCOUNT_SID=

# Twilio Auth Token
TWILIO_AUTH_TOKEN=

# Your Twilio phone number (WhatsApp enabled)
# Format example: whatsapp:+14155238886
TWILIO_PHONE_NUMBER=


# -------------------------------
# Rapid API (YouTube Summarization)
# -------------------------------

# YouTube Video Summarizer GPT API via RapidAPI
# Subscribe & get credentials from: https://rapidapi.com/rahilkhan224/api/youtube-video-summarizer-gpt-ai/playground
RAPIDAPI_HOST=
RAPIDAPI_KEY=
RAPIDAPI_URL=

